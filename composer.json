{
    "name": "syofyanzuhad/artisan-view",
    "description": "Manage your views in Laravel projects through artisan",
    "keywords": ["artisan", "laravel", "views", "templates", "blade"],
    "license": "MIT",
    "authors": [
        {
            "name": "Syofyan Zuhad",
            "email": "syofyanzuhad@gmail.com",
            "homepage": "https://sy.ofyanzuhad.netlify.com"
        }
    ],
    "require": {
        "php": "^7.1 || ^8.0",
        "laravel/framework": "^5.4 || ^6.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5 || ^7.0 || ^8.0",
        "graham-campbell/testbench": "^4.0 || ^5.0",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Syofyanzuhad\\ArtisanView\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Syofyanzuhad\\ArtisanView\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Syofyanzuhad\\ArtisanView\\ServiceProvider"
            ]
        }
    }
}
